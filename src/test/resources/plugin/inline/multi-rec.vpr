field elem: Int
field next: Ref
field right: Ref
field left: Ref

inline predicate list(this: Ref)
{
  acc(this.elem) && acc(this.next) &&
		(this.next != null ==> list(this.next))
}

inline predicate tree(this: Ref)
{
  acc(this.right) && acc(this.left) &&
		(this.right != null ==> tree(this.right)) &&
		(this.left != null ==> tree(this.left))
}