field data: Int


function foo(this: Ref): Bool
  requires acc(this.data)
{ true }

inline predicate bla(this: Ref) {
     acc(this.data) && foo(this)
}

function test02(this: Ref): Int
  requires acc(bla(this), wildcard)
{
  unfolding acc(bla(this), wildcard) in this.data
}